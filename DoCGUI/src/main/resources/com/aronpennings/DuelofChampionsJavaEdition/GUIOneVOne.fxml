            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <effect>
            <Blend />
         </effect>
         <font>
            <Font size="23.0" />
         </font>
      </Button>
      <Button fx:id="returnToLauncher1" layoutX="734.0" layoutY="295.0" mnemonicParsing="false" opacity="0.0" prefHeight="119.0" prefWidth="198.0" style="-fx-background-color: darkred;" text="Exit To Launcher" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
