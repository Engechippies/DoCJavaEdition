<AnchorPane fx:id="background" prefHeight="720.0" prefWidth="1280.0" style="-fx-background-color: black;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.aronpennings.DoCGui.Controllers.GuiOneVOneController">
   <children>
      <TextArea fx:id="textScreen" editable="false" focusTraversable="false" prefHeight="720.0" prefWidth="498.0" text="&#10;" wrapText="true">
         <font>
            <Font size="24.0" />
         </font></TextArea>
      <Button fx:id="returnToLauncher" layoutX="1151.0" layoutY="650.0" mnemonicParsing="false" onAction="#leaveGame" prefHeight="69.0" prefWidth="134.0" style="-fx-background-color: darkred;" text="Return to launcher" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <font>
            <Font size="14.0" />
         </font></Button>
      <Label layoutX="643.0" layoutY="14.0" prefHeight="48.0" prefWidth="117.0" text="Duel Of Champions" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <effect>
            <Glow level="1.0" />
         </effect>
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="startButton" layoutX="498.0" layoutY="600.0" mnemonicParsing="false" onAction="#startGame" prefHeight="119.0" prefWidth="198.0" style="-fx-background-color: lightgreen;" text="Start One v One">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
         <effect>
            <Blend />
         </effect>
         <font>
            <Font size="23.0" />
         </font>
      </Button>
      <Button fx:id="attackButton" layoutX="524.0" layoutY="243.0" mnemonicParsing="false" opacity="0.0" prefHeight="48.0" prefWidth="118.0" text="Attack" />
      <Button fx:id="blockButton" layoutX="524.0" layoutY="307.0" mnemonicParsing="false" opacity="0.0" prefHeight="48.0" prefWidth="118.0" text="Block" />
      <Button fx:id="healButton" layoutX="524.0" layoutY="367.0" mnemonicParsing="false" opacity="0.0" prefHeight="48.0" prefWidth="118.0" text="Heal" />
      <Button fx:id="nothingButton" layoutX="524.0" layoutY="430.0" mnemonicParsing="false" opacity="0.0" prefHeight="48.0" prefWidth="118.0" text="Do Nothing" />
      <Button fx:id="rematchButton" layoutX="952.0" layoutY="295.0" mnemonicParsing="false" opacity="0.0" prefHeight="119.0" prefWidth="198.0" style="-fx-background-color: lightgreen;" text="Rematch">
